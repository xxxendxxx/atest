def determine_anchor_status(row):     if row['系统识别抛锚'] == '是':         description = str(row['故障描述']) + str(row['故障原因'])         static_match = static_pattern.search(description)         moving_match = moving_pattern.search(description)          if moving_match:             print(row['VIN'],'动态',moving_match.group())             return "行驶", moving_match.group()         elif static_match:             print(row['VIN'],'静态',static_match.group())             return "静止", static_match.group()         else:             return "静止", None     return "未知", None  